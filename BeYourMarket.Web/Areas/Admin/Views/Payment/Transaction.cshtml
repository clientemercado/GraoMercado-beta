@{
    ViewBag.Title = "[[[Transações]]]";
}

@section Styles {

}

<div class="wraper container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">[[[Transações]]]</h3>
                </div>

                <div class="panel-body">
                    <div class="alert alert-info">
                        <p>[[[Aqui você pode gerenciar todos os pedidos, você pode cancelar ou aceitar pedidos em nome dos usuários.]]]</p>
                    </div>
                </div>

                @Html.DoAction(HookName.TransactionOverview)

            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/Scripts/gridmvc.min.js"></script>
    @Html.Partial("_LocalizationGridMvc")
}
